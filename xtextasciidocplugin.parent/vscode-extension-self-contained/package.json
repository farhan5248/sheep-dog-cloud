{
    "name": "xtext-asciidoc-plugin",
    "displayName": "Xtext AsciiDoc Plugin",
    "description": "Xtext AsciiDoc Plugin for VS Code",
    "version": "0.0.32",
    "publisher": "xtext",
    "icon": "images/xtext.png",
    "license": "EPL-1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/xtext/xtext-languageserver-example.git"
    },
    "bugs": {
        "url": "https://github.com/xtext/xtext-languageserver-example/issues"
    },
    "engines": {
        "vscode": "^1.103.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:asciidoc"
    ],
    "main": "out/extension",
    "contributes": {
        "languages": [
            {
                "id": "asciidoc",
                "aliases": [
                    "asciidoc"
                ],
                "extensions": [
                    ".asciidoc"
                ],
                "configuration": "./asciidoc.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "asciidoc",
                "scopeName": "text.asciidoc",
                "path": "./syntaxes/asciidoc.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "asciidoc.a.proxy",
                "title": "MyDsl Command A"
            },
            {
                "command": "asciidoc.b",
                "title": "MyDsl Command B"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^20.19.11",
        "@types/vscode": "^1.103.0",
        "typescript": "^5.9.2",
        "@vscode/test-electron": "^2.5.2",
        "@vscode/vsce": "^3.6.0"
    },
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    },
    "scripts": {
        "prepublish": "tsc -p ./src",
        "compile": "tsc -p ./src",
        "watch": "tsc -w -p ./src",
        "update-vscode": "node ./node_modules/vscode/bin/install"
    }
}
